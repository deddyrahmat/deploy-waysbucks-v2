!modalRegister);\r\n\r\n  const [formData, setFormData] = useState({\r\n    email : '',\r\n    password : ''\r\n  })\r\n\r\n  const handleChange = (e) => {\r\n    setFormData({ ...formData, [e.target.name]: e.target.value });\r\n  }\r\n\r\n  const handleSubmit = (e) => {\r\n      e.preventDefault();\r\n\r\n      dispatch({\r\n        type:\"LOGIN\",\r\n        auth : formData\r\n      })\r\n  }\r\n\r\n\r\n  const loginPage = () => {\r\n    toggleLogin();\r\n    toggleRegister();\r\n  }\r\n\r\n  const regisPage = () => {\r\n    toggleLogin();\r\n    toggleRegister();\r\n  }\r\n  \r\n\r\n  return (\r\n    <Fragment>\r\n      {/* form modal login */}\r\n      <NavItem className=\"mt-2\">\r\n          <NavLink>\r\n            <Button outline color=\"danger\" className=\"btn-auth\" onClick={toggleLogin}>Login</Button>\r\n            <Modal isOpen={modalLogin} toggle={toggleLogin} className=\"className\">\r\n              <ModalBody>\r\n                <Container>\r\n                <Form onSubmit={handleSubmit}>\r\n                  <h3 className=\"title-formAuth\">Login</h3>\r\n                  <FormGroup>\r\n                    <Input type=\"email\" name=\"email\" id=\"email\" placeholder=\"Email\" onChange={handleChange} className=\"btn-formAuth\" autoFocus=\"true\" />\r\n                  </FormGroup>\r\n                  <FormGroup>\r\n                    <Input type=\"password\" name=\"password\" id=\"password\" onChange={handleChange} placeholder=\"Password\" className=\"btn-formAuth\" />\r\n                  </FormGroup>\r\n                  \r\n                  <Button block color=\"danger\" className=\"btn-clickAuth\" type=\"submit\" >Login</Button>\r\n                </Form>\r\n                <p className=\"text-center\">Don't have an account ? Klik <b onClick={regisPage}>Here</b> </p>\r\n              </Container>\r\n              \r\n              </ModalBody>\r\n            </Modal>\r\n          </NavLink>\r\n      </NavItem>\r\n      {/* form modal logi